<div class="container">
  <h1>Get in shares your unique high school </h1>
  <br>
  <br>
  <div class="row">
    <div class="col-sm-12 col-md-6 col-lg-8">
     <img id="front_page" src="http://placehold.it/750x450">
    </div>
    <form class="form-horizontal" role="form">
      <div class="form-group">
        <div class="col-sm-12 col-md-6 col-lg-4">
          <!-- <div class="panel-body" id="signup_form"> -->
              <%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
                <%= devise_error_messages! %>
        

                <div class="form-group">
                <%= f.email_field :email, :class => "form-control", :placeholder => "email" %>
                </div>

                <div class="form-group">
                <%= f.password_field :password, :class => "form-control", :placeholder => "password" %>
                </div>


                <div class="form-group">
                <%= f.password_field :password_confirmation, :class => "form-control", :placeholder => "re-enter password" %>
                 </div>

                <%= f.submit "Create my account", :class => "btn btn-large btn-primary" %>
              <% end %>
        </div>
      </div>
    </form>
  </div>
</div>


